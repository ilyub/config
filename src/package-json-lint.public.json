{
  "extends": "./src/package-json-lint.json",
  "rules": {
    "valid-values-author": [
      "error",
      [
        "Ilia Liubinskii <admin.phpdev@gmail.com> (https://github.com/ilyub)"
      ]
    ],
    "valid-values-license": [
